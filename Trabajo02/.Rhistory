simce2m2017_rbd <- read_excel("C:/Users/nachi/OneDrive/Documentos/Universidad/UAH 5° Semestre/OFC R para análisis estadístico/Trabajos/Trabajo02/input/data-orig/simce2m2017_rbd.xlsx")
